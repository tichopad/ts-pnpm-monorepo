{
  "name": "@tpm/template",
  "private": true,
  "version": "1.0.0",
  "description": "Serves as a template for workspace packages",
  "scripts": {
    "format": "prettier --write --cache .",
    "lint": "eslint --fix --cache --ext ts src",
    "build": "tsc --noEmit"
  },
  "eslintConfig": {
    "extends": "@tpm/eslint-config"
  },
  "devDependencies": {
    "@tpm/eslint-config": "workspace:*",
    "@tpm/tsconfig": "workspace:*"
  }
}
